{
  "name": "fanfou-sdk",
  "version": "5.0.0-beta.2",
  "description": "Fanfou SDK for Node.js",
  "type": "module",
  "exports": "./dist/index.js",
  "files": [
    "dist"
  ],
  "dependencies": {
    "camelcase-keys": "^6.2.2",
    "decamelize-keys": "^1.1.0",
    "form-data": "^3.0.0",
    "got": "^10.2.0",
    "he": "^1.1.1",
    "hmacsha1": "^1.0.0",
    "oauth-1.0a": "^2.2.5",
    "query-string": "^6.2.0"
  },
  "devDependencies": {
    "@sindresorhus/tsconfig": "^1.0.2",
    "@types/he": "^1.1.1",
    "del-cli": "^3.0.1",
    "typedoc": "^0.20.36",
    "typescript": "^4.2.4",
    "xo": "^0.38.2"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "del-cli dist docs && tsc && typedoc --gitRevision main source/index.ts",
    "prepublish": "npm run build",
    "pretest": "npm run build",
    "test": "xo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fanfoujs/fanfou-sdk-node.git"
  },
  "keywords": [
    "fanfou"
  ],
  "author": "LitoMore",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fanfoujs/fanfou-sdk-node/issues"
  },
  "homepage": "https://github.com/fanfoujs/fanfou-sdk-node#readme",
  "xo": {
    "prettier": true,
    "rules": {
      "ava/no-ignored-test-files": 0,
      "camelcase": 0
    },
    "ignore": [
      "docs"
    ]
  }
