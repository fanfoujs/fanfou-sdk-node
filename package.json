{
  "name": "fanfou-sdk",
  "version": "5.0.0",
  "description": "Fanfou SDK for Node.js",
  "type": "module",
  "exports": "./distribution/index.js",
  "types": "distribution",
  "files": [
    "distribution"
  ],
  "dependencies": {
    "camelcase-keys": "^7.0.2",
    "decamelize-keys": "^1.1.0",
    "form-data": "^4.0.0",
    "got": "^12.0.4",
    "he": "^1.2.0",
    "hmacsha1": "^1.0.0",
    "oauth-1.0a": "^2.2.6",
    "query-string": "^7.1.1"
  },
  "devDependencies": {
    "@sindresorhus/tsconfig": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/he": "^1.1.2",
    "@types/test-listen": "^1.1.0",
    "ava": "^4.3.3",
    "c8": "^7.12.0",
    "del-cli": "^5.0.0",
    "express": "^4.18.1",
    "gh-pages": "^4.0.0",
    "test-listen": "^1.1.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.14",
    "typescript": "^4.8.3",
    "xo": "^0.52.3"
  },
  "engines": {
    "node": "^14.18.0 || ^16.14.0 || >=18.0.0"
  },
  "scripts": {
    "build": "del-cli distribution && tsc",
    "pretest": "npm run build",
    "test": "xo && c8 ava",
    "typedoc": "npm run build && del-cli docs && typedoc --gitRevision main source/index.ts",
    "deploy:doc": "gh-pages -d docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fanfoujs/fanfou-sdk-node.git"
  },
  "keywords": [
    "fanfou"
  ],
  "author": "LitoMore",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fanfoujs/fanfou-sdk-node/issues"
  },
  "homepage": "https://github.com/fanfoujs/fanfou-sdk-node#readme",
  "xo": {
    "prettier": true,
    "rules": {
      "camelcase": "off",
      "unicorn/no-process-exit": "off",
      "@typescript-eslint/no-implicit-any-catch": "off",
      "@typescript-eslint/no-unsafe-assignment": "off",
      "@typescript-eslint/no-unsafe-member-access": "off",
      "@typescript-eslint/naming-convention": [
        "error",
        {
          "selector": "typeLike",
          "format": [
            "PascalCase"
          ]
        }
      ]
    },
    "ignore": [
      "docs"
    ]
  },
  "ava": {
    "files": [
      "test/*"
    ],
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  }
}
